/* pages/find/find.wxss */
page {
   background-color: #ededed;
}
.title {
   background: -webkit-linear-gradient(left, #6190e8, #a7bfe8);
   height: 180rpx;
   width: 100%;
   position: fixed;
   top: 0;
   z-index: 2;
}

.title .find {
   color: #fff;
   font-size: large;
   position: relative;
   left: 45%;
   top: 55%;
}

.header {
   position: absolute;
   top: 180rpx;
   display: flex;
   width: 100%;
   height: 6%;
   align-items: center;
   background-color: #fff;
   z-index: 2;
}

.header .left {
   width: 68rpx;
   height: 68rpx;
   display: flex;
   align-items: center;
   justify-content: center;
}

.header .right {
   width: 88%;
}

.icon-kuozhan {
   font-size: 40rpx;
}
.container{
   margin-top: 10rpx;
   box-sizing: border-box;
   height: 100vh;
}